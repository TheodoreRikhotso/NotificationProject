<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.notificationproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="com.example.admin.notificationproject.SplashScreen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.admin.notificationproject.LandingScreen" />
        AdminNotification
        <service android:name="com.example.admin.notificationproject.MyFirebaseIntanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.example.admin.notificationproject.MyFirebaseMassagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name="com.example.admin.notificationproject.AdminNotification" />
        NotificationActivity
        <activity android:name="com.example.admin.notificationproject.CatalogAdminActivity" />
        CatalogActivity
        <activity android:name="com.example.admin.notificationproject.NotificationActivity" />
        <activity android:name="com.example.admin.notificationproject.CatalogActivity" />
        <activity
            android:name="com.example.admin.notificationproject.DescriptionActivity"
            android:label="@string/title_activity_description"
            android:theme="@style/AppTheme.NoActionBar" />
        DescriptionActivity
        <activity android:name="com.example.admin.notificationproject.ConfirmRequestActivty" />
        ConfirmRequestActivty
        <activity android:name="com.example.admin.notificationproject.NotificationActivityList" />
        NotificationActivityList
        <activity android:name="com.example.admin.notificationproject.DashBoardActivity" />
        <activity android:name="com.example.admin.notificationproject.ForgotPasswordAcitivity" />
        <activity android:name="com.example.admin.notificationproject.LoginActivity" />
        <activity android:name="com.example.admin.notificationproject.SignUpActivity" />
        <activity android:name="com.example.admin.notificationproject.PhoneActiviy" />
        <activity
            android:name="com.example.admin.notificationproject.CarActivity"
            android:label="Cars" />
        <activity
            android:name="com.example.admin.notificationproject.LaptopActivity"
            android:label="Laptops" />
        <activity
            android:name="com.example.admin.notificationproject.FurnitureActivity"
            android:label="Furniture" />
        <activity android:name="com.example.admin.notificationproject.QAndA" />
        <activity android:name="com.example.admin.notificationproject.ViewActivity" />
        <activity
            android:name="com.example.admin.notificationproject.ProfileActivity"
            android:label="Profile" />
        <activity android:name="com.example.admin.notificationproject.AboutStuffActivity" />
        <activity android:name="com.example.admin.notificationproject.PeopleActivity" />
        <activity android:name="com.example.admin.notificationproject.AboutsActivity" />
        <activity android:name="com.example.admin.notificationproject.AboutImageActivity" />
        <activity android:name="com.example.admin.notificationproject.FAQ" />
        <activity android:name="com.example.admin.notificationproject.PolicyActivity"></activity>
    </application>

</manifest>