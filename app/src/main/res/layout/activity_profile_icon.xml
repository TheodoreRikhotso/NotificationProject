<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.notificationproject.ProfileActivityIcon">




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/imProfile"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:background="@android:color/transparent"
                android:src="@drawable/profile_circular_border_imageview" />

            <TextView
                android:id="@+id/tvUserName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="UserName"
                android:textSize="24dp"
                android:textStyle="bold"
                android:layout_above="@+id/tvUserEmail"
                android:layout_alignStart="@+id/tvUserEmail" />

            <TextView
                android:id="@+id/tvUserEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="UserEmail"
                android:textSize="18dp"
                android:layout_marginBottom="24dp"
                android:layout_marginStart="17dp"
                android:layout_alignBottom="@+id/imProfile"
                android:layout_toEndOf="@+id/imProfile" />

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="0.9dp"
                android:layout_above="@+id/linearLayout2"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="160dp"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/tvAboutUs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageButton"
                android:layout_alignStart="@+id/tvFAQ"
                android:layout_marginBottom="15dp"
                android:text="About us"
                android:textSize="14sp" />

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/information"
                android:layout_alignBottom="@+id/view3"
                android:layout_alignStart="@+id/imageButton6" />


            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="0.9dp"
                android:layout_above="@+id/linearLayout2"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="250dp"
                android:layout_toEndOf="@+id/imageButton"
                android:background="@android:color/darker_gray" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/question_mark"
                android:layout_below="@+id/imageButton"
                android:layout_alignStart="@+id/view2"
                android:layout_marginStart="17dp"
                android:layout_marginTop="19dp" />

            <TextView
                android:id="@+id/tvFAQ"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp"
                android:text="FAQ'S"
                android:textSize="14sp"
                android:layout_alignBottom="@+id/imageButton6"
                android:layout_alignStart="@+id/view4" />

            <View
                android:id="@+id/view4"
                android:layout_width="match_parent"
                android:layout_height="0.9dp"
                android:layout_above="@+id/linearLayout2"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="320dp"
                android:layout_toEndOf="@+id/imageButton6"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/TVContact"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Contact Us"
                android:textSize="14sp"
                android:layout_marginTop="15dp"
                android:layout_alignTop="@+id/imageButton7"
                android:layout_alignStart="@+id/view5" />

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/phone_book"
                android:layout_alignBottom="@+id/view5"
                android:layout_toStartOf="@+id/view5" />

            <View
                android:id="@+id/view5"
                android:layout_width="match_parent"
                android:layout_height="0.9dp"
                android:layout_above="@+id/linearLayout2"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="390dp"
                android:layout_toEndOf="@+id/imageButton6"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/tvLogOut"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="LogOut"
                android:textSize="14sp"
                android:layout_alignBottom="@+id/view6"
                android:layout_alignStart="@+id/TVContact"
                android:layout_marginBottom="12dp" />

            <ImageButton
                android:id="@+id/imageButton8"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/logout001"
                android:layout_alignBottom="@+id/view6"
                android:layout_toStartOf="@+id/view6" />

            <View
                android:id="@+id/view6"
                android:layout_width="match_parent"
                android:layout_height="0.9dp"
                android:layout_above="@+id/linearLayout2"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="460dp"
                android:layout_toEndOf="@+id/imageButton6"
                android:background="@android:color/darker_gray" />

        </RelativeLayout>


    </LinearLayout>




</RelativeLayout>
